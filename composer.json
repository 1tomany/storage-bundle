{
    "name": "1tomany/storage-bundle",
    "description": "Symfony bundle to manage uploading and downloading files to and from remote storage services",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "keywords": [
        "amazon",
        "aws",
        "s3",
        "azure",
        "blob",
        "files",
        "storage",
        "uploads"
    ],
    "require": {
        "php": ">=8.4",
        "ext-fileinfo": "*",
        "psr/container": "^2.0",
        "psr/http-message": "^2.0",
        "symfony/dependency-injection": "^7.2",
        "symfony/filesystem": "^7.2",
        "symfony/http-kernel": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "Provides autowired AWS S3 client for S3 compatible storage services"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\StorageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\StorageBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
